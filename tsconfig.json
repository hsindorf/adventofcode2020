{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"lib": [
			"esnext"
		],
		"strict": true,
		"noImplicitAny": false,
		"sourceMap": true,
		"declaration": false,
		"allowSyntheticDefaultImports": false,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"outDir": "dist",
		"typeRoots": ["node_modules/@types"]
	},
	"include": ["src", "utils"]
}
